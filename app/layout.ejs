<body>
  <div class="header">
    <img src="./assets/images/logo-new_工作區域 1.png" alt="logo">
    <ul class="menu">
      <li><a href="#">系列鏡框</a></li>
      <li><a href="#">門市據點</a></li>
      <li><a href="#">部落格</a></li>
      <li><a href="#">常見問題</a></li>
    </ul>
  </div>

  <%- contents %>

  <!-- 聯繫資訊 -->
  <footer class="footer">
    <div class="container">
      <!-- 導覽列 -->
      <div class="footerNav d-flex justify-content-between">
        <ul class="menu d-flex"> 
          <li><a class="footerNav-item me-5 me-lg-6 me-md-2" href="#">首頁</a></li>
          <li><a class="footerNav-item me-5 me-lg-6 me-md-2" href="#">系列鏡框</a></li>
          <li><a class="footerNav-item me-5 me-lg-6 me-md-2" href="#">門市據點</a></li>
          <li><a class="footerNav-item me-5 me-lg-6 me-md-2" href="#">部落格</a></li>
          <li><a class="footerNav-item me-5 me-lg-6 me-md-2" href="#">常見問題</a></li>
        </ul>
        <ul class="icon d-flex">
          <li><a class="footerNav-item me-5 me-lg-8" href=""><i class="fab fa-facebook"></i></a></li>
          <li><a class="footerNav-item me-5 me-lg-8" href=""><i class="fab fa-instagram"></i></a></li>
          <li><a class="footerNav-item me-5 me-lg-8" href=""><i class="fab fa-line"></i></a></li>
        </ul>
      </div>
      <div class="link">
        <!-- 電話/信箱 -->
        <div class="callMe">
          <ul>
            <li><i class="fas fa-phone-alt me-4 mb-lg-4"></i>0800-000-000</li>
            <li><i class="fas fa-envelope me-4 mb-lg-4"></i>glasses@business.com</li>
          </ul>
        </div>
        <!-- Copyright -->
        <div class="Copyright d-flex mt-4 me-4 justify-content-between">
          <p class="d-flex ">Copyright © 2020 Glasses.All rights reserved.</p>
          <ul class="d-flex">
            <li class="me-4">隱私權政策</li>
            <li>服務條款</li>
          </ul> 
        </div>
      </div>

    </div>
  </footer>


  <script src="./assets/js/vendors.js"></script>
  <script src="./assets/js/all.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.min.js" integrity="sha384-lpyLfhYuitXl2zRZ5Bn2fqnhNAKOAaM/0Kr9laMspuaMiZfGmfwRNFh8HlMy49eQ" crossorigin="anonymous"></script>
